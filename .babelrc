{
  "presets": ["env"],
  "plugins": [
  	"transform-object-rest-spread",
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["transform-class-properties", { "spec": true }]
  ]
}